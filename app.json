{
  "expo": {
    "name": "Nutrition App",
    "slug": "nutrition-app",
    "version": "1.7.1",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4CAF50"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nutritionapp",
      "infoPlist": {
        "NSCameraUsageDescription": "Barkod okutmak için kamera erişimine ihtiyacımız var.",
        "NSMicrophoneUsageDescription": "Bu uygulama mikrofon kullanmaz."
      }
    },
    "android": {
      "package": "com.nutritionapp",
      "versionCode": 7,
      "permissions": [
        "CAMERA",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Barkod okutmak için kamera erişimine ihtiyacımız var."
        }
      ],
      "expo-apple-authentication"
    ],
    "extra": {
      "eas": {
        "projectId": "a36e1ec2-52c9-4a7b-a62f-c5e2b250dc78"
      }
    }
  }
}
